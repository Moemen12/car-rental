{
  "name": "car-rental-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:api-gateway": "nest start api-gateway --watch",
    "start:dev:user-service": "nest start user-service --watch",
    "start:dev:car-service": "nest start car-service --watch",
    "start:dev:rental-service": "nest start rental-service --watch",
    "start:dev:email-service": "nest start email-service --watch",
    "dev:all": "concurrently -n \"API-GATEWAY,CAR-MICROSERVICE,RENTAL-MICROSERVICE,USER-MICROSERVICE,EMAIL-SERVICE\" -c \"yellow,green,blue,magenta,cyan\" \"npm run start:dev:api-gateway\" \"npm run start:dev:car-service\" \"npm run start:dev:rental-service\" \"npm run start:dev:user-service\" \"npm run start:dev:email-service\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/car-rental-project/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/car-rental-project/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.8",
    "@nestjs/mongoose": "^10.1.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/websockets": "^10.4.8",
    "algoliasearch": "^5.15.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.5",
    "cacheable": "^1.8.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.5",
    "countries-list": "^3.1.1",
    "jspdf": "^2.5.2",
    "mongoose": "^8.8.2",
    "nestjs-form-data": "^1.9.92",
    "nodemailer": "^6.9.16",
    "puppeteer": "^23.10.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "string-similarity": "^4.0.4",
    "stripe": "^17.4.0",
    "tesseract.js": "^5.1.1",
    "uploadthing": "^7.4.1",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-local": "^1.0.38",
    "@types/string-similarity": "^4.0.2",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "chalk": "^4.1.2",
    "colors": "^1.4.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@app/microservice-config(|/.*)$": "<rootDir>/libs/microservice-config/src/$1"
    }
  }
}
